<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserInfo">
    <resultMap id="userResultMap" type="com.hgn.kimi.dal.dataobject.UserDO">
        <result property="id" column="id" />
        <result property="name" column="name" />
        <result property="userName" column="user_name" />
        <result property="sex" column="sex" />
        <result property="mobileType" column="mobile_type" />
        <result property="mobileNum" column="mobile_num" />
        <result property="cars" column="cars" />
        <result property="status" column="status" />
        <result property="gmtCreate" column="create_time" />
        <result property="gmtModified" column="modify_time" />
    </resultMap>
	<select id="UserInfo.queryUserInfo"  parameterType="long" resultMap="userResultMap">
		SELECT * FROM user_info
			WHERE id=#{id}
	</select>
	
	
	<insert id="UserInfo.insertUserInfo" parameterType="userDO">
	 <![CDATA[
		INSERT INTO user_info(id,name,sex,mobile_num,mobile_type,user_name,cars,status,create_time,modify_time) VALUES
			(#{id},#{name},#{sex},,#{mobileNum},#{mobileType},#{userName},#{cars},#{status},#{gmtCreate},#{gmtModified})
		]]>
		<selectKey resultType="java.lang.Long" keyProperty="id" >
            SELECT @@IDENTITY AS id
        </selectKey>
 	</insert>
	
	
</mapper>
